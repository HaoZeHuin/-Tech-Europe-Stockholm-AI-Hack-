{
  "name": "@jarvis/contracts",
  "version": "1.0.0",
  "description": "Shared TypeScript contracts and types for Jarvis-in-a-Box",
  "main": "index.ts",
  "types": "index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "contracts",
    "types",
    "voice-assistant",
    "jarvis"
  ],
  "author": "Jarvis Team",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "exports": {
    ".": {
      "import": "./index.ts",
      "require": "./index.ts"
    },
    "./ToolDefs": {
      "import": "./ToolDefs.ts",
      "require": "./ToolDefs.ts"
    },
    "./WebhookContracts": {
      "import": "./WebhookContracts.ts",
      "require": "./WebhookContracts.ts"
    },
    "./RagChunk": {
      "import": "./RagChunk.ts",
      "require": "./RagChunk.ts"
    },
    "./LifeDB": {
      "import": "./LifeDB.ts",
      "require": "./LifeDB.ts"
    }
  }
